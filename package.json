{
  "name": "powerfulyang.com",
  "version": "1.0.0",
  "private": true,
  "description": "home page",
  "scripts": {
    "update:lock": "pnpm i --lockfile-only",
    "bootstrap": "pnpm install --frozen-lockfile",
    "test": "jest",
    "postinstall": "npx husky install",
    "prod:dev": "cross-env CLIENT_BASE_HOST=powerfulyang.com SERVER_BASE_URL=https://powerfulyang.com next dev",
    "qa:dev": "cross-env CLIENT_BASE_HOST=qa.powerfulyang.com SERVER_BASE_URL=https://qa.powerfulyang.com next dev",
    "local:dev": "cross-env SERVER_BASE_URL=https://local.powerfulyang.com next dev",
    "prebuild": "rimraf .next",
    "build": "next build",
    "postbuild": "rimraf --glob ./.next/**/*.map && next-sitemap --config ./next-sitemap.config.mjs",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "tsc-check": "tsc --noEmit",
    "precommit": "lint-staged",
    "prettier": "prettier \"src/**/*.{tsx,ts,scss}\" --write",
    "lint:js": "eslint ./src --ext .ts,.tsx --fix",
    "lint:style": "stylelint --fix \"src/**/*.scss\"",
    "codegen-swagger:prod": "cross-env SWAGGER_ORIGIN=https://api.powerfulyang.com node ./dev/swagger-typescript-codegen.config.mjs"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/powerfulyang/powerfulyang.com"
  },
  "dependencies": {
    "@docsearch/react": "3.3.3",
    "@hookform/resolvers": "3.1.0",
    "@monaco-editor/react": "4.5.0",
    "@powerfulyang/components": "1.6.1",
    "@powerfulyang/hooks": "1.4.0",
    "@powerfulyang/utils": "1.9.2",
    "@react-three/drei": "9.65.3",
    "@react-three/fiber": "8.12.1",
    "@sentry/nextjs": "7.48.0",
    "@tanstack/react-query": "4.29.3",
    "canvas-confetti": "1.6.0",
    "classnames": "2.3.2",
    "d3": "^7.8.4",
    "dayjs": "1.11.7",
    "feed": "4.2.2",
    "framer-motion": "10.12.2",
    "hast-util-raw": "8.0.0",
    "hast-util-to-string": "2.0.0",
    "http-status-codes": "2.2.0",
    "immer": "9.0.21",
    "jotai": "2.0.4",
    "lodash-es": "4.17.21",
    "monaco-editor": "0.37.1",
    "next": "13.3.0",
    "next-seo": "6.0.0",
    "next-sitemap": "4.0.7",
    "peerjs": "1.4.7",
    "ramda": "0.29.0",
    "react": "18.2.0",
    "react-diff-viewer": "3.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.9",
    "react-hotkeys-hook": "4.4.0",
    "react-intersection-observer": "9.4.3",
    "react-markdown": "8.0.7",
    "react-syntax-highlighter": "15.5.0",
    "reflect-metadata": "0.1.13",
    "rehype-katex": "6.0.2",
    "rehype-slug": "5.1.0",
    "remark": "14.0.2",
    "remark-directive": "2.0.1",
    "remark-frontmatter": "4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "5.1.1",
    "remark-rehype": "10.1.0",
    "rxjs": "7.8.0",
    "sharp": "0.31.3",
    "three": "0.151.3",
    "unified": "^10.1.2",
    "unist-util-visit": "4.1.2",
    "yaml": "2.2.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.3.0",
    "@powerfulyang/lint": "3.1.3",
    "@sentry/cli": "2.17.2",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/canvas-confetti": "1.6.0",
    "@types/d3": "7.4.0",
    "@types/lodash-es": "4.17.7",
    "@types/ramda": "0.29.0",
    "@types/react-syntax-highlighter": "15.5.6",
    "@types/three": "0.150.2",
    "jest-canvas-mock": "2.5.0",
    "monaco-editor-webpack-plugin": "7.0.1",
    "next-pwa": "5.6.0",
    "swagger-typescript-api": "12.0.4",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "bundledDependencies": [
    "katex",
    "prettier-plugin-tailwindcss"
  ],
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "18",
        "react-dom": "18"
      }
    },
    "patchedDependencies": {
      "swagger-typescript-api@12.0.4": "patches/swagger-typescript-api@12.0.4.patch"
    }
  }
}
